<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<configuration>
    
    <property require-input="true">
        <name>site.sdk_ws_url</name>
        <value>ws://localhost:8887</value>
        <description>e.g: ws://localhost:8887</description>
    </property>

    <property require-input="true">
        <name>site.collectGateway</name>
        <value>htp://localhost</value>
        <description>e.g: localhost</description>
    </property>

    <property require-input="true">
        <name>site.file_server_url</name>
        <value></value>
        <description>e.g: http://localhost:8765</description>
    </property>

    <property>
        <name>site.file_server_token</name>
        <value></value>
        <description></description>
    </property>

    <property>
        <name>site.file_server_secret</name>
        <value></value>
        <description></description>
    </property>

    <property require-input="true">
        <name>site.websdk_js_cdn</name>
        <value></value>
        <description>e.g: localhost:8080</description>
    </property>

    <property require-input="true">
        <name>db.host</name>
        <value>localhost</value>
        <description></description>
    </property>

    <property require-input="true">
        <name>db.password</name>
        <value>123456</value>
        <property-type>PASSWORD</property-type>
        <description></description>
    </property>

    <property require-input="true">
        <name>redis.host</name>
        <value>host-name</value>
        <description></description>
    </property>

    <property require-input="true">
        <name>dataConfig.hostAndPorts</name>
        <value>hostname:6379</value>
        <description></description>
    </property>

</configuration>
