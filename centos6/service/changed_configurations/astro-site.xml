<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<configuration>
    <property require-input="true">
        <name>site.websdk_app_host</name>
        <value>test1.sugo.vm:8000</value>
        <description>e.g: 192.168.0.120:8000</description>
    </property>

    <property require-input="true">
        <name>site.collectGateway</name>
        <value>http://test2.sugo.vm</value>
        <description>e.g: http://192.168.0.120</description>
    </property>
    <property require-input="true">
        <name>db.host</name>
        <value>test3.sugo.vm</value>
        <description>e.g: 192.168.0.120</description>
    </property>
    <property require-input="true">
        <name>redis.host</name>
        <value>test4.sugo.vm</value>
        <description>e.g: 192.168.0.120</description>
    </property>
    <property require-input="true">
        <name>activeExtendModules</name>
        <value>sugo-analytics-extend-standard</value>
        <description></description>
    </property>
</configuration>

